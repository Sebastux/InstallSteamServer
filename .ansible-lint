exclude_paths:
  - .cache/
  - .github/
  - .yamllint
  - .ansible-lint

verbosity: 1

extra_vars:
  cible: toto

skip_list:
  - 'fqcn-builtins'
  - 'package-latest'

warn_list:
  - 'document-end'
  - 'brackets'
  - 'ignore-errors'
  - 'command-instead-of-shell'
  - 'command-instead-of-module'

use_default_rules: true
