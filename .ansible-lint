exclude_paths:
  - .cache/
  - .github/
  - .yamllint
  - .ansible-lint

verbosity: 1

extra_vars:
  cible: toto

skip_list:
  - 'package-latest'
  - 'fqcn[action]'
  - 'fqcn[action-core]'
  - 'fqcn[action-redirect]'

warn_list:
  - 'document-end'
  - 'brackets'
  - 'ignore-errors'
  - 'command-instead-of-shell'
  - 'command-instead-of-module'
  - 'name[template]'

use_default_rules: true
